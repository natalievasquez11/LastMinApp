{{!-- navigation bar --}}
<ul id="dropdown1" class="dropdown-content">
  <li><a href="/favorites">Favorites List</a></li>
  <li class="divider"></li>
  <li><a href="#!">Sign Out</a></li>
</ul>
<nav class="cyan accent-4">
  <div class="nav-wrapper">
    <a href="/" class="brand-logo">Home<i class="material-icons">home</i></a>
    <ul class="right hide-on-med-and-down">
      <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">UserName<i class="material-icons right">arrow_drop_down_circle</i></a></li>
    </ul>
  </div>
</nav>

{{!--Title--}}
<div class='jumbotron'>
    <h1 class='center-align'>Favorite Events</h1>
</div>
<br>
<div class="container">
    {{!--Events title and filter button--}}
    <div class="row center-align">
        <div class="col s3 offset-s2">
            <h4>Events</h4>
        </div>
    </div>

    {{!--Loop through and display all events in database--}}
    <ul>
        {{#each faves}}
        <li>
            <a href="more-info/{{this.id}}"><img id="eventImg" src='assets/images/dictionary-examples.jpg'></a>
            <div class="eventInfo">
                <a href="more-info/{{this.id}}"><h5>{{this.eventName}}</h5></a>
                <p>{{this.performer}}</p>
                <p>{{this.showType}}</p>
                <p>{{this.price}}</p>
            </div>
        </li>
        {{/each}}
    </ul>
</div>
</div>